    printf("digite o numero de faltas:");
    scanf("%f",&faltas);
    
    presenca=aulas/faltas;
    printf("sua media de presença é:%f ", presenca);
  
  if ( presenca>=75){
  printf("Parabéns,voce foi aprovado:%f ", presenca);
}
