#include <stdio.h>
#include <stdlib.h>
int main() {
float a;
int b;

printf("Digite o valor de a: \n");
scanf("%f", &a);
printf("Digite o valor de b:\n");
scanf("%d", &b);

a -= b; // x = x - 5;
printf("a = %.1f\n", a);
a *= 4; // x = x * 3;
printf("a = %.1f\n", a);
a /= 3; // x = x / 2;
printf("a = %.1f\n", a);
a += 2; // x = x + y;
printf("a = %.1f\n", a);
b %= 5; // y = y % 2;
printf("b = %d\n", b);

return 0;
}
