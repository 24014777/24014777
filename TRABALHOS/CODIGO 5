#include <stdio.h>

int main() {
    int RA, ultimo;

    printf("Digite o seu RA: ");
    scanf("%d", &RA);

    ultimo = RA % 10;

    printf("O último digito do seu RA: %d é: %d\n", RA, ultimo);

    if (ultimo % 2 == 0){
        printf("O seu ultimo digito é PAR.\n");
        } else {
        printf("O seu ultimo digito é ÍMPAR.\n");
}
    return 0;
}
