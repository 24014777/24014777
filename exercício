
include <stdio.h>
int main() {
    int numero_visitantes;

    // Recebe o número de visitantes
    printf("Digite o número de visitantes de hoje: ");
    scanf("%d", &numero_visitantes);

    // Exibe a mensagem de confirmação
    printf("Número de visitantes registrado com sucesso: %d\n", numero_visitantes);

    return 0;
